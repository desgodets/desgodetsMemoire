    empty
  }
tei_figure =
  
  ##  (figure) Regroupe des éléments représentant ou contenant une information graphique comme une illustration ou une figure. [14.4. ]
  element ns1:figure {
    (tei_model.headLike
     | tei_model.common
     | tei_figDesc
     | tei_model.graphicLike
     | tei_model.global
     | tei_model.divBottomPart)*,
    tei_att.global.attributes,
    tei_att.placement.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_figDesc =
  
  ##  (description d'une figure) contient une brève description de l'apparence ou du contenu d'une représentation graphique, pour documenter une image sans avoir à l'afficher [14.4. ]
  element ns1:figDesc {
    tei_macro.limitedContent, tei_att.global.attributes, empty
  }
tei_link =
  
  ##  ( lien) définit une association ou un lien hypertextuel entre des éléments ou des passages, lien dont le type ne peut être spécifié précisément par d'autres éléments. [16.1. ]
  element ns1:link {
    empty
    >> sch:pattern [
         id = "link-constraint-linkTargets1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @targets"
             "You may not supply both \x{a}" ~
             "	@target and @targets on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "link-constraint-linkTargets2"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:report [
             test = "not(@target) and not(@targets)"
             "You must\x{a}" ~
             "	supply either @target or @targets on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "link-constraint-linkTargets3"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "contains(@target,' ')"
             "You must supply at least two\x{a}" ~
             "values for @target on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.pointing.attributes,
    tei_att.typed.attributes,
    
    ## spécifie les identifiants des éléments ou des passages à lier ou à associer.
    attribute targets {
      list { tei_data.pointer, tei_data.pointer, tei_data.pointer* }
    }?,
    empty
  }
tei_linkGrp =
  
  ##  (groupe de liens) définit un ensemble d'associations ou de liens hypertextuels. [16.1. ]
  element ns1:linkGrp {
    (tei_link | tei_ptr)+,
    tei_att.global.attributes,
    tei_att.pointing.group.attributes,
    empty
  }
tei_seg =
  
  ##  (segment quelconque) contient une unité de texte quelconque de niveau segment. [16.3.  6.2.  7.2.5. ]
  element ns1:seg {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    tei_att.responsibility.attributes,
    empty
  }
tei_att.global.linking.attributes =
  tei_att.global.linking.attribute.corresp,
  tei_att.global.linking.attribute.copyOf
tei_att.global.linking.attribute.corresp =
  
  ##  (correspond) pointe vers des éléments qui ont une correspondance avec l'élément en question.
  attribute corresp {
    list { tei_data.pointer+ }
  }?
tei_att.global.linking.attribute.copyOf =
  
  ## pointe vers un élément dont l'élément en question est une copie.
  attribute copyOf { tei_data.pointer }?
tei_model.entryLike = tei_entry
tei_att.entryLike.attributes = tei_att.entryLike.attribute.type
tei_att.entryLike.attribute.type =
  
  ## dans des dictionnaires multi-types, indique le type d'entrée
  ## Les valeurs suggérées comprennent: 1] main; 2] hom (homographe.) ; 3] xref (référence croisée) ; 4] affix; 5] abbr (abréviation.) ; 6] supplemental; 7] foreign
  [ a:defaultValue = "main" ]
  attribute type {
    
    ## une entrée principale (par défaut)
    "main"
    | 
      ##  (homographe.) regroupe des informations relatifs à un homographe dans une entrée.
      "hom"
    | 
      ##  (référence croisée) courte entrée qui ne sert qu’à pointer vers une entrée principale (par exemple, pour les formes d'un verbe irrégulier ou pour des variantes orthographiques : était pointant vers être, ou clef pointant vers clé).
      "xref"
    | 
      ## entrée pour un préfixe, un infixe ou un suffixe.
      "affix"
    | 
      ##  (abréviation.) entrée pour une abréviation.
      "abbr"
    | 
      ## entrée supplémentaire (utilisée dans les dictionnaires qui publient des suppléments contenant des informations mises à jour sur des entrées).
      "supplemental"
    | 
      ## entrée pour un mot étranger dans un dictionnaire monolingue.
      "foreign"
    | xsd:Name
  }?
tei_att.lexicographic.attributes = tei_att.datcat.attributes
tei_entry =
  
  ##  (entrée) contient une entrée structurée de dictionnaire. [9.1.  9.2. ]
  element ns1:entry {
    (tei_sense
     | tei_model.entryPart.top
     | tei_model.global
     | tei_model.ptrLike)+,
    tei_att.global.attributes,
    tei_att.entryLike.attributes,
    tei_att.sortable.attributes,
    empty
  }
tei_sense =
  
  ## regroupe toutes les informations relatives à un des sens d’un mot dans une entrée de dictionnaire (définitions, exemples, équivalents linguistiques, etc.). [9.2. ]
  element ns1:sense {
    (text
     | tei_model.gLike
     | tei_sense
     | tei_model.entryPart.top
     | tei_model.phrase
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## indique le niveau de ce sens dans la hiérarchie.
    attribute level { tei_data.count }?,
    empty
  }
tei_form =
  
  ##  (groupe d'informations sur une forme dans une entrée) regroupe toutes les informations relatives à la morphologie et à la prononciation d'une entrée [9.3.1. ]
  element ns1:form {
    (text
     | tei_model.gLike
     | tei_model.phrase
     | tei_model.inter
     | tei_model.formPart
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## qualifie la forme comme simple, composée, etc.
    ## Les valeurs suggérées comprennent: 1] simple; 2] lemma; 3] variant; 4] compound; 5] derivative; 6] inflected; 7] phrase
    attribute type {
      
      ## item lexical simple
      "simple"
      | 
        ## l'entrée proprement dite
        "lemma"
      | 
        ## variante
        "variant"
      | 
        ## mot formé d'items lexicaux simples
        "compound"
      | 
        ## mot dérivé de l'entrée
        "derivative"
      | 
        ## mot dans une autre forme que la forme habituelle du dictionnaire
        "inflected"
      | 
        ## item lexical à plusieurs mots
        "phrase"
      | xsd:Name
    }?,
    empty
  }
tei_orth =
  
  ##  (forme orthographique) donne l’orthographe d'un mot-vedette de dictionnaire [9.3.1. ]
  element ns1:orth {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## donne le type d’orthographe
    attribute type { tei_data.enumerated }?,
    
    ## donne l'étendue des informations fournies sur l'orthographe.
    ## Exemple de valeurs possibles: 1] full (forme pleine) ; 2] pref (préfixe) ; 3] suff (suffixe) ; 4] part (partiel) 
    [ a:defaultValue = "full" ]
    attribute extent { tei_data.enumerated }?,
    empty
  }
tei_gram =
  
  ##  (information grammaticale ) contient de l'information grammaticale relative à un terme, un mot ou une forme dans une entrée de dictionnaire ou dans un fichier de données terminologiques. [9.3.2. ]
  element ns1:gram {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## classe l'information grammaticale fournie selon une typologie particulière : dans le cas d'informations terminologiques, de préférence au moyen du dictionnaire des types d'éléments de données spécifiés dans la norme ISO 12620.
    ## Exemple de valeurs possibles: 1] pos (partie du discours (chacune des classes de mot
    ##             auxquelles un mot peut appartenir dans une langue donnée, en fonction de la forme, du
    ##             sens ou d'une combinaison de caractéristiques, par exemple nom, verbe, adjectif, etc.) ) ; 2] gen (genre (classification formelle selon laquelle
    ##             les noms et pronoms et souvent les qualificatifs associés sont groupés et fléchis, ou
    ##             prennent une autre forme afin d'exprimer certaines relations syntaxiques)) ; 3] num (nombre (par exemple singulier, pluriel, duel,
    ##             etc.)) ; 4] animate; 5] proper
    attribute type { tei_data.enumerated }?,
    empty
  }
tei_gen =
  
  ##  (genre) identifie le genre morphologique d'un élément lexical, tel qu'il est donné par le dictionnaire. [9.3.1. ]
  element ns1:gen {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    empty
  }
tei_model.morphLike = tei_gram | tei_gen
tei_model.morphLike_alternation = tei_gram | tei_gen
tei_model.morphLike_sequence = tei_gram, tei_gen
tei_model.morphLike_sequenceOptional = tei_gram?, tei_gen?
tei_model.morphLike_sequenceOptionalRepeatable = tei_gram*, tei_gen*
tei_model.morphLike_sequenceRepeatable = tei_gram+, tei_gen+
tei_model.gramPart = tei_model.morphLike | tei_usg | tei_lbl
tei_model.formPart = tei_form | tei_orth | tei_model.gramPart
tei_def =
  
  ##  (définition) contient le texte de la définition dans une entrée de dictionnaire [9.3.3.1. ]
  element ns1:def {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    empty
  }
tei_usg =
  
  ##  (usage) contient, dans une entrée de dictionnaire, les informations sur son usage [9.3.5.2. ]
  element ns1:usg {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## classe les informations sur l'usage en utilisant n'importe quelle typologie adaptée
    ## Exemple de valeurs possibles: 1] geo (zone géographique) ; 2] time; 3] dom (domaine) ; 4] register (registre) ; 5] style; 6] plev (niveau d’usage (principalement, habituellement,
    ##             etc.)) ; 7] lang (langue) ; 8] gram (emploi grammatical ) ; 9] syn (synonyme) ; 10] hyper (hyperonyme) ; 11] colloc (collocation) ; 12] comp (complément) ; 13] obj (objet) ; 14] subj (sujet) ; 15] verb; 16] hint
    attribute type { tei_data.enumerated }?,
    empty
  }
tei_lbl =
  
  ##  (étiquette) étiquette pour la forme d’un mot, pour un exemple, pour une traduction, ou pour tout autre type d’information, par exemple "abréviation pour", "contraction de", "littéralement", "approximativement", "synonymes", etc. [9.3.1.  9.3.3.2.  9.3.5.3. ]
  element ns1:lbl {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## classe l'étiquette en utilisant toute typologie adaptée
    attribute type { tei_data.enumerated }?,
    empty
  }
tei_xr =
  
  ##  (renvoi) contient une expression, une phrase ou une icône qui invite le lecteur à se référer à un autre endroit, dans le même texte ou dans un autre texte [9.3.5.3. ]
  element ns1:xr {
    (text
     | tei_model.gLike
     | tei_model.phrase
     | tei_model.inter
     | tei_usg
     | tei_lbl
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    
    ## indique le type de renvoi, en utilisant n'importe quelle typologie adaptée
    ## Exemple de valeurs possibles: 1] syn (synonyme) ; 2] etym (étymologique) ; 3] cf (comparer ou consulter) ; 4] illus (illustration) 
    attribute type { tei_data.enumerated }?,
    empty
  }
tei_re =
  
  ##  (sous-entrée) contient une entrée relative à un item lexical lié au mot-vedette, tel qu'un composé ou un dérivé, inclus dans une entrée plus large. [9.3.6. ]
  element ns1:re {
    (text
     | tei_model.gLike
     | tei_sense
     | tei_model.entryPart.top
     | tei_model.phrase
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.lexicographic.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_metDecl =
  
  ##  (déclaration sur la métrique) documente la notation utilisée pour noter un modèle métrique lorsque celui-ci est spécifié comme la valeur des attributs met , real, ou rhyme, qui s’appliquent à tout élément de la structure d’un texte versifié (par exemple lg, l, ou seg). [6.5.  6.3. ]
  element ns1:metDecl {
    ((tei_model.pLike | tei_model.noteLike)+ | tei_metSym+),
    tei_att.global.attributes,
    tei_att.declarable.attributes,
    
    ## indique si la notation traduit la forme métrique abstraite, sa réalisation prosodique, le schéma des rimes, ou une combinaison de ces différents éléments.
    [ a:defaultValue = "met real" ]
    attribute type {
      list {
        (
         ##  (attribut met
         ##                ) la déclaration s'applique à la structure métrique abstraite notée par l'attribut met.
         "met"
         | 
           ##  (attribut real
           ##                ) la déclaration s'applique à la réalisation réelle de la structure métrique conventionnelle notée par l'attribut real .
           "real"
         | 
           ##  (attribut rime) la déclaration s'applique à la structure métrique abstraite notée par l'attribut rhyme.
           "rhyme"),
        (
         ##  (attribut met
         ##                ) la déclaration s'applique à la structure métrique abstraite notée par l'attribut met.
         "met"
         | 
           ##  (attribut real
           ##                ) la déclaration s'applique à la réalisation réelle de la structure métrique conventionnelle notée par l'attribut real .
           "real"
         | 
           ##  (attribut rime) la déclaration s'applique à la structure métrique abstraite notée par l'attribut rhyme.
           "rhyme")?,
        (
         ##  (attribut met
         ##                ) la déclaration s'applique à la structure métrique abstraite notée par l'attribut met.
         "met"
         | 
           ##  (attribut real
           ##                ) la déclaration s'applique à la réalisation réelle de la structure métrique conventionnelle notée par l'attribut real .
           "real"
         | 
           ##  (attribut rime) la déclaration s'applique à la structure métrique abstraite notée par l'attribut rhyme.
           "rhyme")?
      }
    }?,
    
    ##  (modèle d'expression régulière) indique une expression régulière définissant toute valeur permise dans cette notation.
    attribute pattern { tei_data.pattern }?,
    empty
  }
tei_metSym =
  
  ##  (Symbole de notation métrique.) documente le sens propre à un caractère particulier ou à une suite de caractères dans une notation métrique, exprimés soit explicitement, soit dans les termes d’autres éléments inclus dans le même élément metDecl. [6.5. ]
  element ns1:metSym {
    tei_macro.phraseSeq.limited,
    tei_att.global.attributes,
    
    ## indique le caractère ou l'ordre des caractères documentés.
    attribute value {
      list { tei_data.word+ }
    },
    
    ## indique si le symbole est défini à l’aide d’autres symboles, (l'attribut terminal a pour valeur false), ou rédigé en texte libre, (l'attribut terminal a pour valeur true).
    [ a:defaultValue = "true" ]
    attribute terminal { tei_data.truthValue }?,
    empty
  }
tei_att.metrical.attributes = empty
tei_att.enjamb.attributes = empty
tei_caesura =
  
  ## signale une coupe rythmique à l'intérieur d'un vers. [6.2. ]
  element ns1:caesura { empty, tei_att.global.attributes, empty }
tei_rhyme =
  
  ## Marque la partie rimante d'une ligne métrique. [6.4. ]
  element ns1:rhyme {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.typed.attributes,
    
    ## Donne une étiquette pour identifier à quelle shéma métrique correspond cette alternance de rimes.
    attribute label { tei_data.word }?,
    empty
  }
tei_s =
  
  ##  (phrase) contient une division textuelle de type phrase [17.1.  8.4.1. ]
