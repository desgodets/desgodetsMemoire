    tei_macro.phraseSeq
    >> sch:pattern [
         id = "s-constraint-noNestedS"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:s"
           "\x{a}" ~
           "               "
           sch:report [
             test = "tei:s"
             "You may not nest one s element within\x{a}" ~
             "      another: use seg instead"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_cl =
  
  ## (clause) propositionreprésente une proposition grammaticale [17.1. ]
  element ns1:cl {
    tei_macro.phraseSeq,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_phr =
  
  ##  (syntagme) représente un syntagme grammatical [17.1. ]
  element ns1:phr {
    tei_macro.phraseSeq,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_w =
  
  ##  (mot) représente un mot grammatical (pas nécessairement orthographique) [17.1. ]
  element ns1:w {
    (text
     | tei_model.gLike
     | tei_seg
     | tei_w
     | tei_m
     | tei_c
     | tei_model.global
     | tei_model.lPart
     | tei_model.hiLike
     | tei_model.pPart.edit)*,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    
    ## fournit le lemme du mot (entrée du dictionnaire)
    attribute lemma { tei_data.text }?,
    
    ## provides a pointer to a definition of the lemma for the word, for example in an online lexicon. 
    attribute lemmaRef { tei_data.pointer }?,
    empty
  }
tei_m =
  
  ##  (morphème) représente un morphème grammatical [17.1. ]
  element ns1:m {
    (text
     | tei_model.gLike
     | tei_seg
     | tei_m
     | tei_c
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    
    ## identifie la forme de base du morphème
    attribute baseForm { tei_data.word }?,
    empty
  }
tei_c =
  
  ##  (caractère) représente un caractère [17.1. ]
  element ns1:c {
    tei_macro.xtext,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_pc =
  
  ##  (punctuation character) contains a character or string of characters regarded as constituting a single punctuation mark. [17.1. ]
  element ns1:pc {
    (text | tei_model.gLike | tei_c | tei_model.pPart.edit)*,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    
    ## indicates the extent to which this punctuation mark conventionally separates words or phrases
    attribute force {
      
      ## the punctuation mark is a word separator
      "strong"
      | 
        ## the punctuation mark is not a word separator
        "weak"
      | 
        ## the punctuation mark may or may not be a word separator
        "inter"
    }?,
    
    ## provides a name for the kind of unit delimited by this punctuation mark.
    attribute unit { tei_data.enumerated }?,
    
    ## indicates whether this punctuation mark precedes or follows the unit it delimits.
    attribute pre { tei_data.truthValue }?,
    empty
  }
tei_att.global.analytic.attributes =
  tei_att.global.analytic.attribute.ana
tei_att.global.analytic.attribute.ana =
  
  ##  (analyse) indique un ou plusieurs éléments contenant des interprétations de l'élément qui porte l'attribut ana.
  attribute ana {
    list { tei_data.pointer+ }
  }?
tei_span =
  
  ## associe une interprétation sous forme d'annotation directement à un passage donné dans un texte. [17.3. ]
  element ns1:span {
    tei_macro.phraseSeq.limited
    >> sch:pattern [
         id = "span-constraint-targetfrom"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@from and @target"
             "\x{a}" ~
             "Only one of the attributes @target and @from may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "span-constraint-targetto"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@to          and          @target"
             "\x{a}" ~
             "Only one of the attributes @target and @to may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "span-constraint-tonotfrom"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@to          and          not(@from)"
             "\x{a}" ~
             "If @to is supplied on "
             sch:name [ ]
             ", @from must be supplied as well"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "span-constraint-tofrom"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "contains(@to,' ') or contains(@from,' ')"
             "\x{a}" ~
             "The attributes @to and @from on "
             sch:name [ ]
             " may each contain only a single value"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.interpLike.attributes,
    tei_att.pointing.attributes,
    
    ## précise le début du passage sur lequel porte l'annotation ; s'il n'est pas accompagné d'un attribut to, désigne alors l'intégralité du passage
    attribute from { tei_data.pointer }?,
    
    ## spécifie la fin du passage annoté.
    attribute to { tei_data.pointer }?,
    empty
  }
tei_spanGrp =
  
  ##  (groupement de fragments de texte) regroupe des éléments span [17.3. ]
  element ns1:spanGrp {
    tei_span*,
    tei_att.global.attributes,
    tei_att.interpLike.attributes,
    empty
  }
tei_interp =
  
  ##  (interprétation) interprétation sous la forme d'une annotation concise, pouvant être liée à un passage dans un texte [17.3. ]
  element ns1:interp {
    (text | tei_model.gLike | tei_model.descLike | tei_model.certLike)*,
    tei_att.global.attributes,
    tei_att.interpLike.attributes,
    empty
  }
tei_interpGrp =
  
  ##  (groupe d'interprétations) regroupe un ensemble d'interprétations ayant en commun une mention de responsabilité ou un type [17.3. ]
  element ns1:interpGrp {
    (tei_model.descLike*, tei_interp+),
    tei_att.global.attributes,
    tei_att.interpLike.attributes,
    empty
  }
tei_att =
  
  ##  (attribut) contient le nom d'un attribut apparaissant dans le courant du texte. [22. ]
  element ns1:att {
    tei_data.name,
    tei_att.global.attributes,
    
    ##  (schéma) fournit l'identifiant du schéma dans lequel ce nom est défini.
    ## Exemple de valeurs possibles: 1] TEI (text encoding initiative) ; 2] DBK(docbook) ; 3] XX (inconnu) 
    [ a:defaultValue = "TEI" ]
    attribute scheme { tei_data.enumerated }?,
    empty
  }
tei_code =
  
  ## contient un code littéral provenant d'un langage formel, comme un langage de programmation.
  element ns1:code {
    text,
    tei_att.global.attributes,
    
    ##  (langage formel) nom identifiant le langage formel dans lequel le code est exprimé
    attribute lang { tei_data.word }?,
    empty
  }
tei_eg =
  
  ##  (exemple) contient toutes sortes d'exemples illustratifs. [22.4.4.  22.4.5. ]
  element ns1:eg { text, tei_att.global.attributes, empty }
tei_egXML =
  
  ##  (exemple en XML) contient un seul exemple en XML bien formé montrant l'utilisation d'un élément ou d'un attribut XML. [22.1.1. ]
  element egXML {
    (text | tei_macro.anyXML)*,
    tei_att.global.attributes,
    tei_att.source.attributes,
    
    ## indicates the intended validity of the example with respect to a schema.
    [ a:defaultValue = "true" ]
    attribute valid {
      
      ## the example is intended to be fully valid, assuming that its root element, or a provided root element, could have been used as a possible root element in the schema concerned.
      "true"
      | 
        ## the example could be transformed into a valid document by inserting any number of valid attributes and child elements anywhere within it; or it is valid against a version of the schema concerned in which the provision of character data, list, element, or attribute values has been made optional.
        "feasible"
      | 
        ## the example is not intended to be valid, and contains deliberate errors.
        "false"
    }?,
    empty
  }
tei_gi =
  
  ##  (nom d'un élément) contient le nom (identifiant générique) d'un élément. [22.  22.4.4. ]
  element ns1:gi {
    tei_data.name,
    tei_att.global.attributes,
    
    ## fournit le nom du modèle dans lequel ce nom est défini
    ## Exemple de valeurs possibles: 1] TEI (cet élément fait partie du modèle TEI.) ; 2] DBK (cet élément fait partie du modèle Docbook.) ; 3] XX (cet attribut fait partie d'un modèle inconnu.) ; 4] Schematron; 5] HTML
    [ a:defaultValue = "TEI" ]
    attribute scheme { tei_data.enumerated }?,
    empty
  }
tei_ident =
  
  ##  (identifiant) contient un identifiant ou un nom dans un langage formel pour un objet quelconque. [22.1.1. ]
  element ns1:ident {
    text, tei_att.global.attributes, tei_att.typed.attributes, empty
  }
tei_tag =
  
  ##  (balise) le contenu d'une balise ouvrante ou fermante, avec éventuellement des spécifications d'attributs, mais à l'exclusion des caractères marquant l'ouverture et la fermeture de la balise. [22. ]
  element ns1:tag {
    text,
    tei_att.global.attributes,
    
    ## indique quel type de balise XML est prévu.
    attribute type {
      
      ## une balise de début, délimitée par les signes <et > est prévue.
      "start"
      | 
        ## une balise de fin, délimitée par les signes </ and > est prévue.
        "end"
      | 
        ## une balise vide, délimitée par les signes < and /> est prévue.
        "empty"
      | 
        ## un PI (Processing Instruction), délimité par les signes <? and ?> est prévu.
        "pi"
      | 
        ## Un commentaire délimité par les signes <!-- et --> est prévu.
        "comment"
      | 
        ## une section marquée, délimitée par les signes <[CDATA[ et ]]> est prévue.
        "ms"
    }?,
    
    ## fournit le nom du modèle dans lequel ce nom est défini
    ## Exemple de valeurs possibles: 1] TEI(text encoding initiative) ; 2] DBK(docbook) ; 3] XX (inconnu) ; 4] Schematron; 5] HTML
    [ a:defaultValue = "TEI" ]
    attribute scheme { tei_data.enumerated }?,
    empty
  }
tei_val =
  
  ##  (valeur) contient une seule valeur d'attribut. [22.  22.4.5. ]
  element ns1:val { text, tei_att.global.attributes, empty }
tei_specList =
  
  ##  (liste de spécification) marque l'endroit où insérer une liste de descriptions dans le texte documentaire.
  element ns1:specList {
    tei_specDesc+, tei_att.global.attributes, empty
  }
tei_specDesc =
  
  ##  (specification description) indique qu'une description de l'élément particulier ou de la classe particulière doit être incluse à ce point dans un document.
  element ns1:specDesc {
    empty,
    tei_att.global.attributes,
    
    ##  (identifiant) donne l'identifiant de l'élément ou de la classe documentaire pour lequels la description est à faire.
    attribute key { tei_data.name }?,
    
    ##  (attributs) donne les noms des attributs dont il faut une description supplémentaire.
    attribute atts {
      list { tei_data.name* }
    }?,
    empty
  }
tei_classRef =
  
  ## points to the specification for an attribute or model class which is to be included in a schema
  element ns1:classRef {
    empty,
    tei_att.global.attributes,
    tei_att.readFrom.attributes,
    
    ## the identifier used for the required class within the source indicated.
    attribute key { xsd:NCName },
    empty
  }
tei_elementRef =
  
  ## points to the specification for some element which is to be included in a schema
  element ns1:elementRef {
    empty,
    tei_att.global.attributes,
    tei_att.readFrom.attributes,
    
    ## the identifier used for the required element within the source indicated.
    attribute key { xsd:NCName },
    empty
  }
tei_macroRef =
  
  ## points to the specification for some pattern which is to be included in a schema
  element ns1:macroRef {
    empty,
    tei_att.global.attributes,
    tei_att.readFrom.attributes,
    
    ## the identifier used for the required pattern within the source indicated.
    attribute key { xsd:NCName },
    empty
  }
tei_moduleRef =
  
  ##  (référence de module) référence un module qui doit être incorporé dans un schéma.
  element ns1:moduleRef {
    (tei_content?)
    >> sch:pattern [
         id = "moduleRef-constraint-modref"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "               "
           sch:report [
             test = "* and @key"
             "\x{a}" ~
             "Child elements of "
             sch:name [ ]
             " are only allowed when an external module is being loaded\x{a}" ~
             "        "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.readFrom.attributes,
    (
     ## specifies a default prefix which will be prepended to all patterns from the imported module
     attribute prefix { xsd:NCName }?)
    >> sch:pattern [
         id = "prefix-constraint-not-same-prefix"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "	              "
           sch:report [
             test =
               "//*[ not( generate-id(.) eq generate-id(      current() ) ) ]/@prefix = @prefix"
             "The prefix attribute\x{a}" ~
             "	    of "
             sch:name [ ]
             " should not match that of any other\x{a}" ~
             "	    element (it would defeat the purpose)"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    (
     ## supplies a list of the elements which are to be copied from the specified module into the schema being defined.
     attribute include {
       list { xsd:NCName* }
     }?
     | 
       ## supplies a list of the elements which are not to be copied from the specified module into the schema being defined.
       attribute except {
         list { xsd:NCName* }
       }?),
    (
     ## le nom d'un module TEI.
     attribute key { xsd:NCName }?
     | 
       ##  (URL) fait référence à un module non TEI de code RELAX NG par une localisation externe.
       attribute url { tei_data.pointer }?),
    empty
  }
tei_moduleSpec =
  
  ##  (spécification de module) documente la structure, le contenu et les fonctions d'un module, i.e. d'un groupe de déclarations nommé et visible extérieurement.
  element ns1:moduleSpec {
    (tei_model.glossLike*,
     tei_model.descLike+,
     tei_exemplum*,
     tei_remarks?,
     tei_listRef*),
    tei_att.global.attributes,
    tei_att.identified.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_schemaSpec =
  
  ##  (spécification de schéma) génère un schéma conforme à la TEI et la documentation qui l'accompagne.
  element ns1:schemaSpec {
    ((tei_model.glossLike | tei_model.descLike)*,
     (tei_model.oddRef | tei_model.oddDecl)*,
     tei_constraintSpec*),
    tei_att.global.attributes,
    tei_att.identified.attributes,
    tei_att.namespaceable.attributes,
    tei_att.readFrom.attributes,
    
    ## précise les points d'accès au schéma, i.e. quels sont les éléments permis comme racine des documents XML qui se conforment à ce schéma.
    [ a:defaultValue = "TEI" ]
    attribute start {
      list { tei_data.name+ }
    }?,
    
    ## précise un préfixe qui sera ajouté à tous les modèles de définition des éléments de la TEI. Cela autorise l'introduction des schémas externes ayant des éléments de même nom que ceux de la TEI
    attribute prefix { "" | tei_data.name }?,
    
    ##  (langue cible) lorsque des noms pour un élément ou pour un attribut sont disponibles en plusieurs langues, précise quelle langue utiliser lors de la création d'objets dans un schéma .
    attribute targetLang { tei_data.language }?,
    
    ##  (langue de documentation) lorsque la description pour un élément, un attribut, une classe ou une macro est disponible en plusieurs langues, précise quelle langue utiliser lors de la création de la documentation.
    attribute docLang {
      list { tei_data.language+ }
    }?,
    empty
  }
tei_specGrp =
  
  ##  (groupe de spécifications) contient tout regroupement approprié de spécifications pour une utilisation dans le module en question.
  element ns1:specGrp {
    (tei_model.oddDecl | tei_model.oddRef | tei_model.divPart)*,
    tei_att.global.attributes,
    empty
  }
tei_specGrpRef =
  
  ##  (référence à un groupe de spécifications) indique que les déclarations contenues dans l'élément specGrpréférencé doivent être insérées à cet endroit.
  element ns1:specGrpRef {
    empty,
    tei_att.global.attributes,
    
    ## pointe vers l'élément specGrp qui doit logiquement être référencé à cet endroit.
    attribute target { tei_data.pointer },
    empty
  }
tei_elementSpec =
  
  ##  (spécification d'élément) documente la structure, le contenu et l'emploi d'un élément. [22.4.4.  22. ]
  element ns1:elementSpec {
    ((tei_model.glossLike | tei_model.descLike)*,
     tei_classes?,
     tei_content?,
     tei_valList?,
     tei_constraintSpec*,
     tei_attList?,
     tei_exemplum*,
     tei_remarks*,
     tei_listRef*)
    >> sch:pattern [
         id = "elementSpec-constraint-elementspec-in-module"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:elementSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "not( @module )                or                //tei:moduleSpec[ @ident = current()/@module ]                or                //tei:moduleRef[ @key = current()/@module ]"
             "\x{a}" ~
             "On element "
             sch:name [ ]
             ' the value of the module attribute ("'
             sch:value-of [ select = "@module" ]
             '")\x{a}' ~
             "should correspond to an existing module, via a moduleSpec or moduleRef"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.identified.attributes,
    tei_att.namespaceable.attributes,
    
    ## specifies a default prefix which will be prepended to all patterns relating to the element, unless otherwise stated. 
    attribute prefix { "" | tei_data.name }?,
    empty
  }
tei_classSpec =
  
  ##  (spécification de classe) contient des informations de référence pour une classe d'éléments TEI, c'est-à-dire un groupe d'éléments qui figurent ensemble dans des modèles de contenu ou qui partagent un attribut commun, ou qui ont l'un et l'autre.
  element ns1:classSpec {
    ((tei_model.glossLike | tei_model.descLike)*,
     tei_classes?,
     tei_constraintSpec*,
     tei_attList?,
     tei_exemplum*,
     tei_remarks*,
     tei_listRef*)
    >> sch:pattern [
         id = "classSpec-constraint-classspec-in-module"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:classSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "not( @module )               or               //tei:moduleSpec[ @ident = current()/@module ]               or               //tei:moduleRef[ @key = current()/@module ]"
             "Class\x{a}" ~
             "      "
             sch:value-of [ select = "@ident" ]
             ': the value of the module attribute ("'
             sch:value-of [ select = "@module" ]
             '") \x{a}' ~
             "should correspond to an existing module, via a moduleSpec or moduleRef"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.identified.attributes,
    
    ## indique si c'est une classe de modèles ou une classe d'attributs
    attribute type {
      
      ##  (modèle de contenu) les membres de cette classe figurent dans les mêmes modèles de contenu
      "model"
      | 
        ##  (attributs) les membres de cette classe partagent des attributs communs
        "atts"
    },
    
    ## donne les règles régissant dans une instance TEI l'ordre et l'alternance des éléments définis par la classe. Par défaut, toutes les variations sont données.
    attribute generate {
      list {
        (
         ## les membres de la classe constituent des alternatives
         "alternation"
         | 
           ## les membres de la classe doivent tous être donnés dans l'ordre indiqué
           "sequence"
         | 
           ## les membres de la classe peuvent être donnés, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptional"
         | 
           ## les membres de la classe peuvent être donnés une ou plusieurs fois, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptionalRepeatable"
         | 
           ## les membres de la classe doivent être donnés au moins une fois, dans l'ordre indiqué.
           "sequenceRepeatable"),
        (
         ## les membres de la classe constituent des alternatives
         "alternation"
         | 
           ## les membres de la classe doivent tous être donnés dans l'ordre indiqué
           "sequence"
         | 
           ## les membres de la classe peuvent être donnés, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptional"
         | 
           ## les membres de la classe peuvent être donnés une ou plusieurs fois, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptionalRepeatable"
         | 
           ## les membres de la classe doivent être donnés au moins une fois, dans l'ordre indiqué.
           "sequenceRepeatable")?,
        (
         ## les membres de la classe constituent des alternatives
         "alternation"
         | 
           ## les membres de la classe doivent tous être donnés dans l'ordre indiqué
           "sequence"
         | 
           ## les membres de la classe peuvent être donnés, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptional"
         | 
           ## les membres de la classe peuvent être donnés une ou plusieurs fois, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptionalRepeatable"
         | 
           ## les membres de la classe doivent être donnés au moins une fois, dans l'ordre indiqué.
           "sequenceRepeatable")?,
        (
         ## les membres de la classe constituent des alternatives
         "alternation"
         | 
           ## les membres de la classe doivent tous être donnés dans l'ordre indiqué
           "sequence"
         | 
           ## les membres de la classe peuvent être donnés, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptional"
         | 
           ## les membres de la classe peuvent être donnés une ou plusieurs fois, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptionalRepeatable"
         | 
           ## les membres de la classe doivent être donnés au moins une fois, dans l'ordre indiqué.
           "sequenceRepeatable")?,
        (
         ## les membres de la classe constituent des alternatives
         "alternation"
         | 
           ## les membres de la classe doivent tous être donnés dans l'ordre indiqué
           "sequence"
         | 
           ## les membres de la classe peuvent être donnés, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptional"
         | 
           ## les membres de la classe peuvent être donnés une ou plusieurs fois, dans l'ordre indiqué, mais sont facultatifs
           "sequenceOptionalRepeatable"
         | 
           ## les membres de la classe doivent être donnés au moins une fois, dans l'ordre indiqué.
           "sequenceRepeatable")?
      }
    }?,
    empty
  }
tei_macroSpec =
  
  ##  (spécification de macro.) documente la fonction et l'implémentation d'un modèle.
  element ns1:macroSpec {
    ((tei_model.glossLike | tei_model.descLike)*,
     (tei_content | tei_valList)*,
     tei_constraintSpec*,
     tei_exemplum*,
     tei_remarks*,
     tei_listRef*)
    >> sch:pattern [
         id = "macroSpec-constraint-macrospec-in-module"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:macroSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "not( @module )               or               //tei:moduleSpec[ @ident = current()/@module ]               or               //tei:moduleRef[ @key = current()/@module ]"
             "Macro "
             sch:value-of [ select = "@ident" ]
             ': the value of the module attribute  ("'
             sch:value-of [ select = "@module" ]
             '")\x{a}' ~
             "should correspond to an existing module, via a moduleSpec or moduleRef"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.identified.attributes,
    
    ## indique quel type d'entité doit être généré lorsqu'un processeur "ODD" génère un module TEI qui utilise la syntaxe XML DTD.
    attribute type {
      
      ##  (entité paramètre) 
      "pe"
      | 
        ##  (entité type de données) 
        "dt"
    }?,
    empty
  }
tei_remarks =
  
  ##  (remarques) contient tout commentaire sur l'utilisation d'un élément, d'un attribut, d'une classe ou d'une entité qui n'est pas documentée ailleurs à l'intérieur de l'élément conteneur. [22.4.4.  22.4.5.  22.4.6.  22.4.7. ]
  element ns1:remarks {
    tei_model.pLike+,
    tei_att.global.attributes,
    tei_att.translatable.attributes,
    empty
  }
tei_listRef =
  
  ##  (liste de références) fournit une liste de références signifiantes là où cet élément est commenté, dans le document courant ou ailleurs.
  element ns1:listRef { tei_ptr+, tei_att.global.attributes, empty }
tei_exemplum =
  
  ##  (exemple) contient un exemple qui montre l'utilisation d'un élément avec de possibles paragraphes de commentaires. [22.4.4. ]
  element ns1:exemplum {
    (tei_model.pLike*, (tei_egXML | tei_eg), tei_model.pLike*),
    tei_att.global.attributes,
    tei_att.typed.attributes,
    tei_att.translatable.attributes,
    empty
  }
tei_classes =
  
  ##  (classes) précise toutes les classes dont la classe ou l'élément documentés est un membre ou une sous-classe. [22.4.4.  22.4.6. ]
  element ns1:classes {
    tei_memberOf*,
    tei_att.global.attributes,
    
    ## indique l'effet de cette déclaration sur son module parent.
    [ a:defaultValue = "replace" ]
    attribute mode {
      
      ## cette déclaration modifie la déclaration de même nom dans la définition courante.
      "change"
      | 
        ## cette déclaration remplace la déclaration de même nom dans la définition courante.
        "replace"
    }?,
    empty
  }
tei_memberOf =
  
  ## précise à quelle classe appartiennent la classe ou l'élément parent.
  element ns1:memberOf {
    tei_macro.xtext,
    tei_att.global.attributes,
    
    ## précise l’identifiant pour une classe pour laquelle l'élément documenté ou classe est un membre ou sous-classe. 
    attribute key { tei_data.name }?,
    
    ## indique l'effet de cette déclaration sur son module parent.
    [ a:defaultValue = "add" ]
    attribute mode {
      
      ## cette déclaration s'ajoute aux définitions courantes.
      "add"
      | 
        ## cette déclaration et tous ses enfants sont retirés du système courant.
        "delete"
    }?,
    
    ## supplies the maximum number of times the element can occur in elements which use this model class in their content model
    attribute max { tei_data.numeric }?,
    
    ## supplies the minumum number of times the element must occur in elements which use this model class in their content model
    attribute min { tei_data.numeric }?,
    empty
  }
tei_equiv =
  
  ##  (équivalent) précise un composant considéré comme un équivalent de l'élément parent, soit par une référence commune, soit par un lien externe. [22.4.4.  22.4.5.  22.4.6.  22.4.7. ]
  element ns1:equiv {
    empty,
    tei_att.global.attributes,
    tei_att.internetMedia.attributes,
    
    ## nomme le concept sous-jacent dont le parent est une représentation.
    attribute name { tei_data.name }?,
    
    ##  (Identifiant de ressource uniforme.) référence le concept sous-jacent dont le parent est une représentation au moyen d'un identifiant externe quelconque.
    attribute uri { tei_data.pointer }?,
    
    ## référence un script externe qui contient une méthode pour transformer les instances de cet élément en TEI canonique.
    attribute filter { tei_data.pointer }?,
    empty
  }
tei_altIdent =
  
  ##  (identifiant alternatif) fournit le nom XML recommandé pour un élément, une classe, un attribut, etc. dans un langage quelconque.
  element ns1:altIdent {
    text, tei_att.global.attributes, tei_att.typed.attributes, empty
  }
tei_content =
  
  ##  (modèle de contenu) contient la déclaration d'un modèle de contenu pour le schéma documenté. [22.4.4. ]
  element ns1:content {
    tei_macro.schemaPattern*,
    tei_att.global.attributes,
    
    ## controls whether or not pattern names generated in the corresponding RELAXNG schema source are automatically prefixed to avoid potential nameclashes. 
    [ a:defaultValue = "true" ]
    attribute autoPrefix {
      
      ## Each name referenced in e.g. a rng:ref element within a content model is automatically prefixed by the value of the prefix attribute on the current schemaSpec
      "true"
      | 
        ## No prefixes are added: any prefix required by the value of the prefix attribute on the current schemaSpec must therefore be supplied explicitly, as appropriate.
        "false"
    }?,
    empty
  }
tei_constraint =
  
  ##  (constraint rules) the formal rules of a constraint
  element ns1:constraint {
    (text | tei_macro.anyXML), tei_att.global.attributes, empty
  }
tei_constraintSpec =
  
  ##  (constraint on schema) contains a constraint, expressed in some formal syntax, which cannot be expressed in the structural content model [22.4.4. ]
  element ns1:constraintSpec {
    tei_model.descLike*,
    tei_model.glossLike*,
    (tei_constraint?)
    >> sch:ns [
         prefix = "s"
         uri = "http://www.ascc.net/xml/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-sch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/s:* and    not(@scheme='schematron')"
             "\x{a}" ~
             "	Rules in the Schematron 1.* language must be inside\x{a}" ~
             "	a constraint with a value of 'schematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:ns [
         prefix = "sch"
         uri = "http://purl.oclc.org/dsdl/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-isosch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/sch:* and    not(@scheme='isoschematron')"
             "\x{a}" ~
             "	Rules in the ISO Schematron language must be inside\x{a}" ~
             "	a constraint with a value of 'isoschematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.identified.attributes,
    tei_att.typed.attributes,
    
    ## supplies the name of the language in which the constraints are defined
    attribute scheme {
      
      ##  (Schematron) 
      "schematron"
      | 
        ##  (ISO Schematron) 
        "isoschematron"
      | 
        ##  (XSLT) 
        "xsl"
      | 
        ##  (private constraint language) 
        "private"
    },
    empty
  }
tei_attList =
  
  ##  (liste d'attributs) contient la documentation pour tous les attributs associés à cet élément comme une série d'éléments attDef. [22.4.4.  22.4.6. ]
  element ns1:attList {
    (tei_attRef | tei_attDef | tei_attList)+,
    tei_att.global.attributes,
    
    ##  (conditions d'utilisation) précise si les attributs dans la liste sont tous disponibles (org="group") ou seulement l'un d'entre eux (org="choice"). 
    [ a:defaultValue = "group" ]
    attribute org {
      
      ## tous les attributs de la liste sont disponibles. 
      "group"
      | 
        ## un seul des attributs de la liste est disponible. 
        "choice"
    }?,
    empty
  }
tei_attDef =
  
  ##  (définition d'attribut) contient la définition d'un attribut. [22.4.5. ]
  element ns1:attDef {
    ((tei_model.glossLike | tei_model.descLike)*,
     tei_datatype?,
     tei_constraintSpec*,
     tei_defaultVal?,
     (tei_valList | tei_valDesc+)?,
     tei_exemplum*,
     tei_remarks*)
    >> sch:pattern [
         id = "attDef-constraint-attDefContents"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:attDef"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "   @mode eq 'change'       or @mode eq 'delete'       or tei:datatype       or tei:valList[@type='closed']       "
             "Attribute: the definition of the @"
             sch:value-of [ select = "@ident" ]
             " attribute in the "
             sch:value-of [ select = "ancestor::*[@ident][1]/@ident" ]
             "\x{a}" ~
             "                  "
             sch:value-of [ select = "' '" ]
             "\x{a}" ~
             "                  "
             sch:value-of [
               select = "local-name(ancestor::*[@ident][1])"
             ]
             " should have a closed valList or a datatype"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.identified.attributes,
    
    ## précise qu'un attribut ou un élément sont facultatifs.
    [ a:defaultValue = "opt" ]
    attribute usage {
      
      ##  (requis) 
      "req"
      | 
        ##  (obligatoire si c'est applicable) 
        "mwa"
      | 
        ##  (recommandé) 
        "rec"
      | 
        ##  (recommandé si c'est applicable) 
        "rwa"
      | 
        ##  (facultatif) 
        "opt"
    }?,
    
    ##  (espace de noms) précise l'espace de noms auquel appartient cet attribut.
    [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
    attribute ns { tei_data.namespace }?,
    empty
  }
tei_attRef =
  
  ##  (pointeur d'attribut) pointe vers la définition d'un attribut ou d'un groupe d'attributs.
  element ns1:attRef {
    empty,
    tei_att.global.attributes,
    
    ## le nom du modèle définissant le(s) attribut(s).
    attribute name { tei_data.word },
    empty
  }
tei_datatype =
  
  ##  (type de données) précise la valeur déclarée d'un attribut en faisant référence à un type de données défini dans le langage choisi pour le schéma. [1.4.2.  22.4.5. ]
  element ns1:datatype {
    tei_macro.schemaPattern*,
    tei_att.global.attributes,
    
    ##  (nombre minimum d'occurrences) indique le nombre minimum d'occurrences de ce type de données dans la spécification de l'attribut.
    [ a:defaultValue = "1" ] attribute minOccurs { tei_data.count }?,
    
    ##  (nombre maximum d'occurrences.) indique le nombre maximum d'occurrences de ce type de données dans la spécification de l'attribut.
    [ a:defaultValue = "1" ]
    attribute maxOccurs { tei_data.count | "unbounded" }?,
    empty
  }
tei_defaultVal =
  
  ##  (valeur par défaut) précise la valeur déclarée par défaut pour un attribut. [22.4.5. ]
  element ns1:defaultVal { text, tei_att.global.attributes, empty }
tei_valDesc =
  
  ##  (description de la valeur) précise toute contrainte sémantique ou syntaxique sur la valeur que peut prendre un attribut, en supplément de l'information portée par l'élément datatype. [22.4.5. ]
  element ns1:valDesc {
    tei_macro.phraseSeq.limited,
    tei_att.global.attributes,
    tei_att.translatable.attributes,
    tei_att.combinable.attributes,
    empty
  }
tei_valItem =
  
  ## documente une valeur d'attribut unique dans une liste d'items possibles ou obligatoires [22.4.5. ]
  element ns1:valItem {
    (tei_model.descLike | tei_model.glossLike)*,
    tei_att.global.attributes,
    tei_att.combinable.attributes,
    
    ## specifies the value concerned.
    attribute ident { text },
    empty
  }
tei_valList =
  
  ##  (liste de valeurs) contient un ou plusieurs éléments valItem qui définissent des valeurs possibles pour un attribut. [22.4.5. ]
  element ns1:valList {
    tei_valItem*,
    tei_att.global.attributes,
    tei_att.combinable.attributes,
    
    ## précise l'extensibilité de la liste des valeurs de l'attribut.
    [ a:defaultValue = "open" ]
    attribute type {
      
      ## seules les valeurs indiquées sont autorisées.
      "closed"
      | 
        ##  (semi-ouvert ) toutes les valeurs indiquées doivent être acceptées, mais d'autres valeurs sont acceptables et le logiciel doit avoir une procédure qui leur est adaptée.
        "semi"
      | 
        ## les valeurs indiquées ne sont que des valeurs d'exemple.
        "open"
    }?,
    empty
  }
tei_att.combinable.attributes = tei_att.combinable.attribute.mode
tei_att.combinable.attribute.mode =
  
  ## précise l'effet de cette déclaration sur son parent.
  [ a:defaultValue = "add" ]
  attribute mode {
    
    ## cette déclaration s'ajoute aux définitions courantes.
    "add"
    | 
      ## cette déclaration et tous ses enfants sont retirés du système courant.
      "delete"
    | 
      ## cette déclaration modifie la déclaration de même nom dans la définition courante.
      "change"
    | 
      ## cette déclaration remplace la déclaration de même nom dans la définition courante.
      "replace"
  }?
tei_att.identified.attributes =
  tei_att.combinable.attributes,
  tei_att.identified.attribute.ident,
  tei_att.identified.attribute.status
tei_att.identified.attribute.ident =
  
  ## fournit l'identifiant qui référence cet élément.
  attribute ident { tei_data.name }
tei_att.identified.attribute.status =
  
  ## indicates the current status of the object identified with respect to the current version of the TEI Guidelines.
  [ a:defaultValue = "stable" ]
  attribute status {
    
    ## the item is not recommended for use, and may be withdrawn at a future release.
    "deprecated"
    | 
      ## the item is new and still under review.
      "unstable"
    | 
      ## the item has changed significantly since the preceding version.
      "changed"
    | 
      ## the item has not recently changed and is not expected to do so except for correction of any errors.
      "stable"
  }?
tei_att.namespaceable.attributes = tei_att.namespaceable.attribute.ns
tei_att.namespaceable.attribute.ns =
  
  ##  (espace de noms) précise l'espace de noms auquel appartient cet élément.
  [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
  attribute ns { tei_data.namespace }?
start = tei_TEI | tei_teiCorpus
