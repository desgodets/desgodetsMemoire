tei_row =
  
  ##  (rangée) contient une rangée d'un tableau. [14.1.1. ]
  element ns1:row {
    tei_cell+,
    tei_att.global.attributes,
    tei_att.tableDecoration.attributes,
    empty
  }
tei_cell =
  
  ##  (cellule) contient une cellule d'un tableau. [14.1.1. ]
  element ns1:cell {
    tei_macro.paraContent,
    tei_att.global.attributes,
    tei_att.tableDecoration.attributes,
    empty
  }
tei_formula =
  
  ##  (formule) contient une formule mathématique ou tout autre type de formule [14.2. ]
  element ns1:formula {
    (text | tei_model.graphicLike | tei_model.hiLike)*,
    tei_att.global.attributes,
    
    ## précise le nom d'une notation définie précédemment, utilisée dans le contenu de l'élément.
